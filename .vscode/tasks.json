{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build scanner",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-g",
                "-Wall",
                "-Wextra",
                "scanner.c",
                "reader.c",
                "token.c",
                "error.c",
                "charcode.c",
                "-o",
                "${workspaceFolder}/incompleted/output/scanner.exe"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"]
        }
    ]
}
